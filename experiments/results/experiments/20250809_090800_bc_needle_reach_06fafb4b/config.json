{
  "algorithm": "bc",
  "seed": 42,
  "network": {
    "needle_reach": {
      "hidden_sizes": [
        256,
        256
      ],
      "activation": "relu"
    }
  },
  "training": {
    "needle_reach": {
      "epochs": 5,
      "batch_size": 64,
      "learning_rate": 0.0001,
      "weight_decay": 0.0001
    }
  },
  "data": {
    "num_demonstrations": 100
  },
  "logging": {
    "tensorboard": true,
    "csv_logging": true,
    "log_interval": 10,
    "save_freq": 20,
    "verbose": 1
  }
}